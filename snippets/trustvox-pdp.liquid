{% if product.id and product.id != 0 %}

<script type="text/javascript">
    window._trustvox =  [];
    
    
    _trustvox.push(['_storeId', 118866 ]);
    
    _trustvox.push(['_productId',  {{ product.id }} ]);
    _trustvox.push(['_productName', "{{ product.title }}" ]);
    {% comment %} _trustvox.push(['_productPhotos', [url.image]]); {% endcomment %}
    
    
</script>
<script async="true" type="text/javascript" src="{{ 'trustvox_sincero.js' | asset_url }}"></script>
{% comment %} <script async="true" type="text/javascript" src="//static.trustvox.com.br/trustvox-sincero-staging/sincero.js"></script> {% endcomment %}
{% endif %}
